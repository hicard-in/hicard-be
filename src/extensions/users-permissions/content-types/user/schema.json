{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "userId": {
      "type": "uid",
      "required": true
    },
    "theme": {
      "type": "string"
    },
    "color": {
      "type": "string"
    },
    "isActivated": {
      "type": "boolean"
    },
    "contact_info": {
      "type": "component",
      "repeatable": false,
      "component": "contact-info.contact-info"
    },
    "bio": {
      "type": "component",
      "repeatable": false,
      "component": "profile.bio"
    },
    "name": {
      "type": "string"
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "banner": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "social_links": {
      "displayName": "social_links",
      "type": "component",
      "repeatable": false,
      "component": "social-links.social-links"
    },
    "payment": {
      "displayName": "payment",
      "type": "component",
      "repeatable": false,
      "component": "payment.payment"
    },
    "productivity": {
      "displayName": "productivity",
      "type": "component",
      "repeatable": false,
      "component": "productivity.productivity"
    }
  }
}
